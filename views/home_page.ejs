<!DOCTYPE HTML>
<html>
    <head>
      <!--Links the CSS file and get work sans font family-->
        <link rel="stylesheet" type="text/css" href="/CSS/home_page.css" />
        <link href="https://fonts.googleapis.com/css2?family=Work+Sans&display=swap" rel="stylesheet">
        <link rel="shortcut icon" type="image/x-icon" href="/Images/water.svg" />
        <title>Mentality</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
    </head>
    <body>
        
      <script>
          //Functions used to link buttons to different pages
          function visitPage_Edu(){
          window.location="/education";
            }
           function vistPage_Med(){
             window.location="/meditation"
           } 
           function vistPage_Wri(){
             window.location="/writing"
           } 
           function vistPage_About(){
            window.location="/about"
           }
           function vistPage_Contact(){
             window.location="/contact"
           }
           function vistPage_Home(){
             window.location="/"
           }
           function vistPage_Support(){
             window.location="/support"
           }

            
      </script>
        <!--Navbar-->
        <ul class="topnav">
          <li><a class="active" onclick="vistPage_Home()">Home</a></li>
          <li><a onclick="vistPage_Support()">Support</a></li>
          <li><a onclick="vistPage_Contact()">Contact</a></li>
          <li class="right"><a onclick="vistPage_About()">About</a></li>
        </ul>
        
    
          

          <div class="parallax-background">
            <h1>Mentality</h1>
          </div>
            <!--Layer 1 of parallax effect-->
            <div class="layer_1">
              <!--This div holds th grid container of the main buttons-->
              <div class="grid-container">
                <!--Description of what the buttons do-->
                <p>Click a button, where do you want to go?</p>
                <div class="grid-item">
                  <button id="button_M" class="buttons", onclick="vistPage_Med();">Meditation</button>
                </div>
                <div class="grid-item">
                  <button id="button_W" class="buttons", onclick="vistPage_Wri();"">Writing</button>
                </div>
                <div class="grid-item">
                  <button id="button_E" class="buttons", onclick="visitPage_Edu();">Education</button>
                </div> 
              </div>
            </div>
            <div class="parallax-background"></div>
        
            
    </body>
</html>
